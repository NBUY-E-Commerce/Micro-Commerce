

@model List<ProductModel>

@if (Model != null && Model.Count > 0)
{
    <div class="dropdown-content" id="info">

        @foreach (var item in Model)
        {


            <a href="#about">@item.ProductName</a>


        }

    </div>
}
else
{
    <div class="dropdown-content" id="info">


        Veri bulunamadı
    </div>

}
